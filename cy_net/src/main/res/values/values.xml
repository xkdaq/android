<?xml version="1.0" encoding="utf-8"?>
<resources>
    <eat-comment />
    <bool name="pickerview_customTextSize">true</bool>
    <eat-comment />
    <color name="bgColor_overlay">#60000000</color>
    <color name="pickerview_bg_topbar">#ffffff</color>
    <color name="pickerview_timebtn_nor">#333333</color>
    <color name="pickerview_timebtn_pre">#d5d5d5</color>
    <color name="pickerview_topbar_title">#000000</color>
    <color name="pickerview_wheelview_textcolor_center">#333333</color>
    <color name="pickerview_wheelview_textcolor_divider">#EEEEEE</color>
    <color name="pickerview_wheelview_textcolor_out">#888888</color>
    <declare-styleable name="wheelview">
        <attr name="gravity">
            <enum name="center" value="17" />
            <enum name="left" value="3" />
            <enum name="right" value="5" />
        </attr>
        <attr name="textSize" format="dimension" />
        <attr name="textColorOut" format="color" />
        <attr name="textColorCenter" format="color" />
        <attr name="dividerColor" format="color" />
    </declare-styleable>
    <eat-comment />
    <dimen name="pickerview_textsize">18sp</dimen>
    <dimen name="pickerview_topbar_btn_textsize">14sp</dimen>
    <dimen name="pickerview_topbar_height">48dp</dimen>
    <dimen name="pickerview_topbar_paddingleft">10dp</dimen>
    <dimen name="pickerview_topbar_paddingright">30dp</dimen>
    <dimen name="pickerview_topbar_title_textsize">18sp</dimen>
    <eat-comment />
    <integer name="animation_default_duration">300</integer>
    <eat-comment />
    <string name="pickerview_cancel">取消</string>
    <string name="pickerview_day">日</string>
    <string name="pickerview_hours">时</string>
    <string name="pickerview_minutes">分</string>
    <string name="pickerview_month">月</string>
    <string name="pickerview_seconds">秒</string>
    <string name="pickerview_submit">确定</string>
    <string name="pickerview_year">年</string>
</resources>